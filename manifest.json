{
  "manifest_version": 3,
  "name": "Content Scraper",
  "version": "1.0",
  "description": "Pulls content from the current page.",
  "permissions": ["scripting", "activeTab", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}
